{
  "name": "restaurant-landing-page",
  "version": "1.0.0",
  "description": "DelishDine Restaurant Landing Page",
  "main": "index.html",
  "scripts": {
    "dev": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "serve": "node dev-server.js",
    "start": "npm run dev & npm run serve",
    "build": "tailwindcss -i ./src/input.css -o ./style.css --watch",
    "build:prod": "tailwindcss -i ./src/input.css -o ./style.css --minify"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2"
  },
  "keywords": [
    "restaurant",
    "landing-page",
    "tailwind",
    "responsive"
  ],
  "author": "Your Name",
  "license": "MIT"
}
